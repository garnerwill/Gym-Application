@{
  @ViewBag.Title = "Map";      
}
    <style>
         /* Always set the map height explicitly to define the size of the div
        * element that contains the map. */
         #map {
             height: 400px;
             width:400px;
             }   
         /* Optional: Makes the sample page fill the window. */
         
    </style>
    <div class="row">
        <div id="map"></div>
        <script>
            var map;
            function initMap() {
                map = new google.maps.Map(document.getElementById("map"), {
                    center: { lat: -34.397, lng: 150.644 },
                    zoom: 8
                });
            }
        </script>
        <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAY94W50Ro6315b5RFNT3TTUKMd3DsUrEU&callback=initMap"
                async defer></script>
    </div>